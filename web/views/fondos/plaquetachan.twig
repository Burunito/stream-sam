/* OBJECTS */
.background-wrap-1 {
    width: 1280px !important;
    height: 720px !important;
    position: absolute;
    background-size: contain;
    background-repeat: no-repeat;
    background-color: #14151E;
    background-image: url('../../images/plaquetachan1.png');
}
.background-wrap-2 {
    width: 1280px !important;
    height: 720px !important;
    position: absolute;
    background-size: cover;
    background-repeat: no-repeat;
    background-image: url('../../images/plaquetachan4.png');
}

